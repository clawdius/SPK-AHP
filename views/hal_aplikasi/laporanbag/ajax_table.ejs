<table class="table table-striped table-hover" data-pagination="true" data-search="true" data-toggle="table" data-locale="id-ID" data-pagination-parts='[pageInfo, pageList]' data-buttons="table-buttons">
    <thead>
        <tr>
            <th scope="col" data-sortable="true" data-width="50">No</th>
            <th scope="col" data-sortable="true">Nama Calon Karyawan</th>
            <th scope="col" data-sortable="true">Email</th>
            <th scope="col" data-sortable="true">No.Telp</th>
            <th scope="col" data-sortable="true">Nilai Total</th>
            <th scope="col" data-sortable="true">Status MBR</th>
        </tr>
    </thead>
    <tbody>
        <% let i = 0; for(data of rank[0]){ %>
            <tr>
                <td>
                    <%= i+1 %>
                </td>
                <td>
                    <%= data.NAMA_CALON %>
                </td>
                <td>
                    <%= data.EMAIL %>
                </td>
                <td>
                    <%= data.NO_TELP %>
                </td>
                <td>
                    <%= +(rank[1][data.ID_CALON]) %>
                </td>
                <td>
                    <%= (data.STAT_MBR == 0)? 'Tidak Berpenghasilan Kecil' : 'Berpenghasilan Kecil' %>
                </td>
            </tr>
            <%i++;}%>
    </tbody>
</table>